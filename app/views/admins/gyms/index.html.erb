<div class="web-location">
  <h1><%= @web_location %></h1>
  <%= link_to "Add new", new_admins_gym_path, :class => "waves-effect waves-light btn right" %>
</div>
<div class="main-content">
  <div id="admin-gym">
    <table class="highlight">
      <thead>
        <tr>
          <th>Image</th>
          <th>Name</th>
          <th>#</th>
        </tr>
      </thead>
      <tbody>
      <% @gyms.each do |gym| %>
        <tr>
          <td><%= image_tag gym.image.url(:medium), :width => "100" %></td>
          <td><%= link_to gym.name, gym_path(gym.slug), :target => "_blank" %></td>
          <td>
            <%= link_to "View", gym_path(gym.slug), :target => "_blank" %>
            <%= link_to "Edit", edit_admins_gym_path(gym) %>
            <%= link_to "Delete", admins_gym_path(gym), method: :delete, data: {confirm: 'Are you sure?'} %>
          </td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>
